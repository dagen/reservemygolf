{
    "name":"reservemygolf",
    "openBrowser":true,
    "startTimeout":240,
    "stopsocket":50123,
    "console":true,
    "profile":"development",
    "dockEnable":true,
    "trayEnable":true,
    "jvm":{
        "heapSize":512,
        "minHeapSize":256
    },
    "openBrowserURL":"http://localhost:8080/",
    "web":{
        "http":{
            "enable":"true",
            "port":"8080"
        },
        "rewrites":{
            "enable":"true"
        },
        "host":"127.0.0.1",
        "webroot":"src/web",
        "directoryBrowsing":false,
        "accessLogEnable":true,
        "maxRequests":30,
        "gzipEnable":true,
        "welcomeFiles":"index.cfm, main.cfm, go.cfm"
    }
}
